<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.guns.rest.persistence.dao.MtimeYearDictTMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.stylefeng.guns.rest.persistence.model.MtimeYearDictT">
        <id column="UUID" property="uuid" />
        <result column="show_name" property="showName" />
    </resultMap>

    <select id="getYears" resultType="com.stylefeng.guns.rest.persistence.model.bo.filmBo.YearDict">
        SELECT
            mtime_year_dict_t.UUID AS yearId,
            mtime_year_dict_t.show_name AS yearName
        FROM
            mtime_year_dict_t
    </select>
</mapper>
