<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.guns.rest.persistence.dao.MtimeCatDictTMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.stylefeng.guns.rest.persistence.model.bo.filmBo.MtimeCatDictT">
        <id column="UUID" property="uuid" />
        <result column="show_name" property="showName" />
    </resultMap>


    <select id="getCats" resultType="com.stylefeng.guns.rest.persistence.model.bo.filmBo.CatDict">
        SELECT
            mtime_cat_dict_t.UUID AS catId,
            mtime_cat_dict_t.show_name AS catName
        FROM
            mtime_cat_dict_t
    </select>
    <select id="searchById" resultType="string">
        select show_name from mtime_cat_dict_t where UUID=#{uuid}
    </select>
</mapper>
